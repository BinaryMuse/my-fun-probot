{
  "name": "my-fun-probot",
  "version": "1.0.0",
  "description": "A test Probot app",
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "ISC",
  "repository": "https://github.com//my-fun-probot.git",
  "homepage": "https://github.com//my-fun-probot",
  "bugs": "https://github.com//my-fun-probot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon -w src/**/* -e js,ts,mjs,json --exec \"npm start\"",
    "start": "ts-node ./node_modules/probot/bin/probot-run ./src/index.ts",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@binarymuse/tsmonad": "^0.8.1",
    "@types/nock": "^9.3.0",
    "nock": "^10.0.0",
    "probot": "^7.2.0",
    "probot-metadata": "BinaryMuse/metadata#97747f3",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
